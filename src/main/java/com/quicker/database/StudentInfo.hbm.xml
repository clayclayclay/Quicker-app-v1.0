<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.quicker.database.StudentInfo" table="student"
		 catalog="quicker">
		<id name="id">
			<column name="id" sql-type="int unsigned" not-null="true" />
			<generator class="identity" />
		</id>
		<property name="username">
			<column name="username" sql-type="varchar" length="13" />
		</property>
		<property name="password">
			<column name="password" sql-type="varchar" length="16" />
		</property>
		<property name="grade">
			<column name="grade" sql-type="varchar" length="2" />
		</property>
		<property name="college">
			<column name="college" sql-type="varchar" length="10" />
		</property>
		<property name="major">
			<column name="major" sql-type="varchar" length="10" />
		</property>
		<property name="classs">
			<column name="classs" sql-type="varchar" length="10" />
		</property>
		<property name="position">
			<column name="position" sql-type="varchar" length="10" />
		</property>
		<property name="isAssigned">
			<column name="isAssigned" sql-type="int" length="1" />
		</property>
		<property name="phone">
			<column name="phone" sql-type="varchar" length="11" />
		</property>
		<property name="name">
			<column name="name" sql-type="varchar" length="12" />
		</property>
		
	</class>
</hibernate-mapping>